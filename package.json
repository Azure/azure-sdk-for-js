{
  "name": "@azure/monorepo",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.12.1",
  "engines": {
    "pnpm": "^9.0.0"
  },
  "scripts": {
    "build:lage": "lage build",
    "build": "pnpm -r --aggregate-output --reporter=append-only build",
    "build:pnpm:parallel": "pnpm -r --workspace-concurrency=Infinity --aggregate-output --reporter=append-only build",
    "clean": "pnpm -r run clean",
    "cspell": "cspell --no-progress .",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "format:dir": "prettier --write",
    "preinstall": "npx only-allow pnpm",
    "purge": "rimraf --glob \"sdk/**/node_modules/\""
  },
  "devDependencies": {
    "@vitest/browser": "^2.1.2",
    "@vitest/coverage-istanbul": "^2.1.2",
    "cspell": "^8.15.1",
    "lage": "^2.11.9",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.10",
    "tsx": "^4.19.1",
    "turbo": "^2.1.3",
    "typescript": "~5.5.4",
    "vitest": "^2.1.2"
  }
}
