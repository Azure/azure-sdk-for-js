{
  "name": "@azure/eslint-plugin-azure-sdk",
  "version": "3.0.0",
  "description": "An ESLint plugin enforcing design guidelines for the JavaScript/TypeScript Azure SDK",
  "sdk-type": "utility",
  "private": true,
  "keywords": [
    "eslint",
    "eslint-plugin",
    "azure",
    "azure-sdk",
    "sdk",
    "eslint-plugin-azure",
    "eslint-plugin-azure-sdk"
  ],
  "author": "Microsoft Corporation",
  "license": "MIT",
  "homepage": "https://github.com/Azure/azure-sdk-for-js/tree/main/common/tools/eslint-plugin-azure-sdk/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Azure/azure-sdk-for-js.git",
    "directory": "common/tools/eslint-plugin-azure-sdk"
  },
  "bugs": {
    "url": "https://github.com/Azure/azure-sdk-for-js/issues"
  },
  "type": "module",
  "//metadata": {
    "constantPaths": [
      {
        "path": "src/utils/constants.ts",
        "prefix": "SDK_VERSION"
      }
    ]
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "browser": {
        "types": "./dist/browser/index.d.ts",
        "default": "./dist/browser/index.js"
      },
      "react-native": {
        "types": "./dist/react-native/index.d.ts",
        "default": "./dist/react-native/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    },
    "./prettier.json": "./prettier.json"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && tshy",
    "build:samples": "echo Skipped.",
    "check-format": "prettier --list-different --config ../../../.prettierrc.json --ignore-path ../../../.prettierignore \"./**/*.{ts,json,md}\"",
    "clean": "rimraf --glob dist* types",
    "format": "prettier --write --config ../../../.prettierrc.json --ignore-path ../../../.prettierignore \"./**/*.{ts,json,md}\"",
    "lint": "eslint src tests/rules",
    "lint:fix": "eslint src tests/rules --fix --fix-type [problem,suggestion]",
    "pack": "pnpm pack 2>&1",
    "test": "npm run test:node && npm run test:browser",
    "test:browser": "echo skipped",
    "test:node": "vitest",
    "update-snippets": "echo skipped"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "prettier": "./prettier.json",
  "peerDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "~8.43.0",
    "@typescript-eslint/parser": "~8.43.0",
    "eslint": "^9.9.0",
    "eslint-plugin-n": "^17.15.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-tsdoc": "^0.4.0"
  },
  "dependencies": {
    "@eslint/compat": "^1.2.4",
    "@eslint/js": "^9.9.0",
    "@types/eslint": "^9.6.0",
    "@types/estree": "~1.0.0",
    "@typescript-eslint/typescript-estree": "~8.43.0",
    "eslint-config-prettier": "^10.0.1",
    "glob": "^11.0.3",
    "tslib": "^2.6.2",
    "typescript": "~5.8.2",
    "typescript-eslint": "~8.43.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.0",
    "@types/eslint-config-prettier": "6.11.3",
    "@types/node": "catalog:",
    "@typescript-eslint/eslint-plugin": "~8.43.0",
    "@typescript-eslint/parser": "~8.43.0",
    "@typescript-eslint/rule-tester": "~8.43.0",
    "@typescript-eslint/utils": "~8.43.0",
    "@vitest/browser": "catalog:testing",
    "@vitest/coverage-istanbul": "catalog:testing",
    "cross-env": "catalog:",
    "eslint": "catalog:",
    "playwright": "catalog:testing",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tshy": "catalog:",
    "vitest": "catalog:testing"
  },
  "tshy": {
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts",
      "./prettier.json": "./prettier.json"
    },
    "dialects": [
      "esm",
      "commonjs"
    ],
    "esmDialects": [
      "browser",
      "react-native"
    ],
    "selfLink": false
  },
  "browser": "./dist/browser/index.js",
  "main": "./dist/commonjs/index.js",
  "types": "./dist/commonjs/index.d.ts",
  "module": "./dist/esm/index.js"
}
