{
  "name": "repo-health-status-report",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Script to generate a report of the health status of this repository.",
  "prettier": "../../../common/tools/eslint-plugin-azure-sdk/prettier.json",
  "scripts": {
    "build": "npm run format && npm run lint && npm run typecheck && npm run test",
    "format": "prettier --write --config ../../../.prettierrc.json --ignore-path ../../../.prettierignore \"src/**/*.ts\" \"test/**/*.ts\"",
    "typecheck": "tsc",
    "run-tool": "tsx src/index.ts",
    "lint": "eslint src test",
    "test": "vitest"
  },
  "dependencies": {
    "@azure/identity": "^4.9.1",
    "dotenv": "^16.5.0",
    "glob":"^11.0.2",
    "octokit": "^4.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/node": "^18.0.0",
    "@vitest/coverage-istanbul": "^3.0.9",
    "eslint": "^9.26.0",
    "prettier": "3.3.3",
    "tsx": "^4.19.4",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.32.0",
    "vitest": "^3.0.9"
  }
}
