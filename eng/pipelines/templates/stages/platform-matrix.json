{
  "displayNames": {
    "**/test-results.xml": ""
  },
  "matrix": {
    "Agent": {
      "windows-2022": {
        "OSVmImage": "env:WINDOWSVMIMAGE",
        "Pool": "env:WINDOWSPOOL"
      },
      "ubuntu-20.04": {
        "OSVmImage": "env:LINUXVMIMAGE",
        "Pool": "env:LINUXPOOL"
      },
      "macos-latest": {
        "OSVmImage": "env:MACVMIMAGE",
        "Pool": "env:MACPOOL"
      }
    },
    "NodeTestVersion": ["18.x", "20.x", "22.4.x"],
    "TestType": "node",
    "TestResultsFiles": "**/test-results.xml"
  },
  "include": [
    {
      "Agent": {
        "windows-2022": {
          "OSVmImage": "env:WINDOWSVMIMAGE",
          "Pool": "env:WINDOWSPOOL"
        }
      },
      "Scenario": {
        "coverage": {
          "TestType": "node",
          "TestResultsFiles": "**/test-results.xml",
          "PublishCodeCoverage": "true"
        },
        "sample": {
          "TestType": "sample",
          "TestResultsFiles": "**/test-results.xml"
        },
        "browser": {
          "TestType": "browser",
          "TestResultsFiles": "**/test-results.browser.xml"
        }
      },
      "NodeTestVersion": "18.x"
    },
    {
      "Agent": {
        "ubuntu-20.04": {
          "OSVmImage": "env:LINUXVMIMAGE",
          "Pool": "env:LINUXPOOL"
        }
      },
      "TestType": "node",
      "NodeTestVersion": "18.x",
      "DependencyVersion": ["max", "min"],
      "TestResultsFiles": "**/test-results.xml"
    }
  ]
}
