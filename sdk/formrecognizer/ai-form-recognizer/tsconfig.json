{
  "extends": "../../../tsconfig",
  "compilerOptions": {
    "preserveConstEnums": true,
    "module": "NodeNext",
    "paths": {
      "@azure/ai-form-recognizer": [
        "./src/index"
      ]
    },
    "skipLibCheck": true,
    "moduleResolution": "NodeNext",
    "rootDir": "."
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.mts",
    "src/**/*.cts",
    "samples-dev/**/*.ts",
    "test/**/*.ts",
    "test/**/*.mts",
    "test/**/*.cts"
  ]
}
