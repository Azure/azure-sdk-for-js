  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Current JS file",
      "runtimeExecutable": "node",
      "runtimeArgs": ["--nolazy", "-r", "@azure/dev-tool/register"],
      "skipFiles": [
        "<node_internals>/**"
      ],
      "args": ["${file}"],
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}