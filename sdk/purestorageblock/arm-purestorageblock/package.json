      "./package.json": "./package.json",
      ".": "./src/index.ts",
      "./api": "./src/api/index.ts",
      "./api/avsVmVolumes": "src/api/avsVmVolumes/index.ts",
      "./api/avsVms": "src/api/avsVms/index.ts",
