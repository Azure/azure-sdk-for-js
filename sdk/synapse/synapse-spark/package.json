{
  "name": "@azure/synapse-spark",
  "author": "Microsoft Corporation",
  "description": "Azure client library for Azure Synapse Spark.",
  "version": "1.0.0-beta.1",
  "dependencies": {
    "@azure/abort-controller": "^1.0.0",
    "@azure/core-http": "^1.1.6",
    "tslib": "^2.0.0",
    "@opentelemetry/api": "^0.10.2",
    "@azure/core-auth": "^1.1.3",
    "@azure/core-tracing": "1.0.0-preview.9",
    "@azure/logger": "^1.0.0",
    "@azure/core-paging": "^1.1.1"
  },
  "keywords": [
    "node",
    "azure",
    "typescript",
    "synapse",
    "spark"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist-esm/src/index.js",
  "types": "./types/synapse-spark.d.ts",
  "devDependencies": {
    "@azure/identity": "^1.1.0",
    "@azure/test-utils-recorder": "^1.0.0",
    "typescript": "~3.9.3",
    "rollup": "^1.16.3",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-shim": "^1.0.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-visualizer": "^4.0.4",
    "uglify-js": "^3.4.9",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-json": "^4.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@microsoft/api-extractor": "7.7.11",
    "@rollup/plugin-multi-entry": "^3.0.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-coverage": "^2.0.0",
    "karma-edge-launcher": "^0.4.2",
    "karma-env-preprocessor": "^0.1.1",
    "karma-json-preprocessor": "^0.3.3",
    "karma-json-to-file-reporter": "^1.0.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-mocha": "^2.0.1",
    "karma-remap-istanbul": "^0.6.0",
    "karma": "^5.1.0",
    "mocha-junit-reporter": "^1.18.0",
    "mocha": "^7.1.1",
    "@types/chai": "^4.1.6",
    "@types/mocha": "^7.0.2",
    "@types/node": "^8.0.0",
    "@types/sinon": "^9.0.4",
    "assert": "^1.4.1",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.1.0",
    "inherits": "^2.0.3",
    "prettier": "^1.16.4",
    "rimraf": "^3.0.0",
    "@azure/dev-tool": "^1.0.0",
    "@azure/eslint-plugin-azure-sdk": "^3.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@types/fs-extra": "^8.0.0",
    "@types/query-string": "6.2.0",
    "esm": "^3.2.18",
    "fs-extra": "^8.1.0",
    "nyc": "^14.0.0",
    "puppeteer": "^3.3.0",
    "query-string": "^5.0.0",
    "sinon": "^9.0.2",
    "source-map-support": "^0.5.9",
    "url": "^0.11.0"
  },
  "homepage": "https://github.com/Azure/azure-sdk-for-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Azure/azure-sdk-for-js.git"
  },
  "bugs": {
    "url": "https://github.com/Azure/azure-sdk-for-js/issues"
  },
  "files": [
    "types/synapse-spark.d.ts",
    "dist/",
    "dist-browser/",
    "dist-esm/src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "audit": "node ../../../common/scripts/rush-audit.js && rimraf node_modules package-lock.json && npm i --package-lock-only 2>&1 && npm audit",
    "build": "npm run extract-api && npm run build:nodebrowser",
    "build:test": "tsc -p . && rollup -c rollup.test.config.js 2>&1",
    "build:node": "tsc -p . && cross-env ONLY_NODE=true rollup -c 2>&1",
    "build:browser": "tsc -p . && cross-env ONLY_BROWSER=true rollup -c 2>&1",
    "build:nodebrowser": "rollup -c 2>&1",
    "check-format": "prettier --list-different \"src/**/*.ts\" \"test/**/*.ts\" \"*.{js,json}\"",
    "clean": "rimraf dist dist-* types *.tgz *.log",
    "extract-api": "tsc -p . && api-extractor run --local",
    "format": "prettier --write --config ../../../.prettierrc.json --ignore-path ../../../.prettierignore \"src/**/*.ts\" \"test/**/*.ts\" \"*.{js,json}\"",
    "generate-doc": "api-documenter markdown -i temp -o docGen",
    "lint:fix": "eslint package.json tsconfig.json api-extractor.json src test --ext .ts --fix --fix-type [problem,suggestion]",
    "lint": "eslint package.json tsconfig.json api-extractor.json src test --ext .ts -f html -o synapse-spark-lintReport.html || exit 0",
    "test:browser": "npm run clean && npm run build:test && npm run unit-test:browser",
    "test:node": "npm run clean && npm run build:test && npm run unit-test:node",
    "test": "npm run clean && npm run build:test && npm run unit-test",
    "unit-test:browser": "karma start --single-run",
    "unit-test:node": "mocha --reporter ../../../common/tools/mocha-multi-reporter.js dist-test/index.node.js --timeout 500000",
    "unit-test": "npm run unit-test:node && npm run unit-test:browser",
    "pack": "npm pack 2>&1",
    "prebuild": "npm run clean"
  },
  "sideEffects": false
}
