    "nock": "^13.5.4",
    "nyc": "catalog:",
    "rimraf": "catalog:",
    "sinon": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  },
  "dependencies": {
    "@azure/core-auth": "workspace:*",
    "@azure/core-client": "workspace:*",
    "@azure/core-rest-pipeline": "workspace:*",
    "@azure/logger": "workspace:*",
    "@azure/monitor-opentelemetry-exporter": "1.0.0-beta.25",
    "@azure/opentelemetry-instrumentation-azure-sdk": "^1.0.0-beta.7",
    "@microsoft/applicationinsights-web-snippet": "^1.2.1",
    "@opentelemetry/api": "catalog:",
    "@opentelemetry/api-logs": "catalog:",
    "@opentelemetry/core": "catalog:",
    "@opentelemetry/instrumentation": "catalog:",
    "@opentelemetry/instrumentation-bunyan": "catalog:",
