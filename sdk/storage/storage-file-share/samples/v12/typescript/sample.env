# Used in most samples. Retrieve these values from a storage account in the Azure Portal.
ACCOUNT_NAME=<storage-account-name>

# Used for dynamic credential fetching via Azure Resource Manager (alternative to ACCOUNT_KEY, ACCOUNT_SAS, or STORAGE_CONNECTION_STRING)
SUBSCRIPTION_ID=<azure-subscription-id>
RESOURCE_GROUP=<resource-group-name>

# Used for sharedKeyAuth sample. Can be omitted if SUBSCRIPTION_ID and RESOURCE_GROUP are set.
ACCOUNT_KEY=<storage-account-key>

# Used for connectionStringAuth sample. Can be omitted if SUBSCRIPTION_ID and RESOURCE_GROUP are set.
STORAGE_CONNECTION_STRING=<storage-account-connection-string>

# Used for anonymousAuth sample. Can be omitted if SUBSCRIPTION_ID and RESOURCE_GROUP are set.
ACCOUNT_SAS=<shared-access-signature>

# Used for listHandles sample, provide an existing share and directory name.
SHARE_NAME=<share name>
DIR_NAME=<directory name>

# To run the proxyAuth sample, set up an HTTP proxy and enter your information:
# HTTP_PROXY=http://localhost:3128
# HTTPS_PROXY=http://localhost:3128
