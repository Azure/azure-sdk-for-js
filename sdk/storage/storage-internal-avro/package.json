  },
  "main": "./dist/index.js",
  "module": "dist-esm/src/index.js",
  "keywords": [
    "azure",
    "cloud"
  ],
  "browser": {
    "./dist-esm/storage-internal-avro/src/index.js": "./dist-esm/storage-internal-avro/src/index.browser.js",
