    "dist-esm/storage-blob/src/utils/",
    "types/latest/storage-file-datalake.d.ts",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "repository": "github:Azure/azure-sdk-for-js",
  "keywords": [
