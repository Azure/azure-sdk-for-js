  "paths": {
    "/localNumbers/costs": {
      "get": {
        "tags": ["10DLC"],
        "summary": "Retrieves a collection of eligible local number costs with pagination.",
        "description": "This method supports pagination via the \"skip\" and \"top\" query parameters.",
        "operationId": "10DLC_GetCosts",
        "produces": ["application/json"],
        "parameters": [
          {
            "in": "query",
            "name": "skip",
            "description": "The number of items to skip in the result set (default: 0).",
            "type": "integer",
            "format": "int32",
            "default": 0
          },
          {
            "in": "query",
            "name": "top",
            "description": "The maximum number of items to return in the result set (default: 100).",
            "type": "integer",
            "format": "int32",
            "default": 100
          },
          {
            "$ref": "#/parameters/ApiVersionParameter"
          }
        ],
        "responses": {
          "200": {
            "description": "Returns the paginated collection of eligible local number costs.",
            "schema": {
              "$ref": "#/definitions/LocalNumberCosts"
            }
          },
          "default": {
            "description": "Failure",
            "schema": {
              "$ref": "#/definitions/CommunicationErrorResponse"
            }
          }
        },
        "x-ms-pageable": {
          "nextLinkName": "nextLink",
          "itemName": "costs"
        },
        "x-ms-examples": {
          "List local numbers costs": {
            "$ref": "./examples/Get10DlcCostsAsync.json"
          }
        }
      }
    },
