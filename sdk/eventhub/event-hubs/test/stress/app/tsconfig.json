{
  "compilerOptions": {
    "target": "es5",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "lib": ["ESNext.AsyncIterable"],
    "outDir": "dist",
    "rootDir": ".",
    "strict": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": ["./src/*.ts"],
  "exclude": ["node_modules"]
}
