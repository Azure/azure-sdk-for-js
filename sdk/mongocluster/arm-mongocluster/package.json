      },
      "require": {
        "types": "./dist/commonjs/api/privateLinks/index.d.ts",
        "default": "./dist/commonjs/api/privateLinks/index.js"
      }
    }
  },
  "main": "./dist/commonjs/index.js",
  "types": "./dist/commonjs/index.d.ts"
}
