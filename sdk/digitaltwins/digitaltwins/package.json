{
  "name": "@azure/digitaltwins",
  "version": "1.0.0-preview.1",
  "description": "Azure NodeJs client library for Azure Digital Twins",
  "sdk-type": "client",
  "author": "Microsoft Corporation",
  "license": "MIT",
  "main": "dist/digitalTwinsClient.js",
  "module": "dist-esm/index.js",
  "types": "types/digitalTwinsClient.d.ts",
  "scripts": {
    "clean": "rimraf node_modules dist dist-esm test-dist temp types *.tgz *.log",
    "prepack": "npm install && npm run build",
    "pack": "npm pack 2>&1",
    "build": "tsc -p . && rollup -c 2>&1 && api-extractor run --local",
    "extract-api": "tsc -p . && api-extractor run --local"
  },
  "files": [
    "dist/",
    "dist-esm/",
    "types/digitalTwinsClient.d.ts"
  ],
  "keywords": [
    "azure",
    "iot",
    "digitaltwins",
    "twins",
    "node",
    "typescript",
    "browser",
    "isomorphic"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/azure/azure-sdk-for-js.git"
  },
  "homepage": "https://github.com/Azure/azure-sdk-for-js/tree/master/sdk/digitaltwins/",
  "bugs": {
    "url": "https://github.com/azure/azure-sdk-for-js/issues"
  },
  "dependencies": {
    "@azure/core-arm": "~1.0.0-preview.7",
    "@azure/core-http": "^1.1.3",
    "@azure/core-paging": "^1.0.0",
    "@azure/identity": "^1.0.3",
    "@types/uuid": "^8.0.0",
    "tslib": "^1.9.3",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "7.7.11",
    "@types/mocha": "^7.0.2",
    "mocha": "^7.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.16.3",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "@rollup/plugin-commonjs": "11.0.2",
    "typescript": "^3.9.6"
  },
  "sideEffects": false
}
