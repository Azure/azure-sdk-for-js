{
  "name": "@azure/batch",
  "author": "Microsoft Corporation",
  "description": "BatchServiceClient Library with typescript type definitions for node.js and browser.",
  "version": "8.0.0",
  "dependencies": {
    "@azure/ms-rest-azure-js": "^2.0.1",
    "@azure/ms-rest-js": "^2.0.4",
