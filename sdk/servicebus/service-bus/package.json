{
  "name": "@azure/service-bus",
  "sdk-type": "client",
  "author": "Microsoft Corporation",
  "version": "7.10.0",
  "license": "MIT",
  "description": "Azure Service Bus SDK for JavaScript",
  "homepage": "https://github.com/Azure/azure-sdk-for-js/tree/main/sdk/servicebus/service-bus/",
  "repository": "github:Azure/azure-sdk-for-js",
  "keywords": [
    "azure",
    "cloud",
    "service bus",
    "AMQP",
    "azure"
  ],
  "bugs": {
    "url": "https://github.com/Azure/azure-sdk-for-js/issues"
  },
  "main": "./dist/index.js",
  "module": "dist-esm/src/index.js",
  "browser": "./dist/browser/index.js",
  "types": "./types/latest/service-bus.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "audit": "node ../../../common/scripts/rush-audit.js && rimraf node_modules package-lock.json && npm i --package-lock-only 2>&1 && npm audit",
    "bundle": "tsc -p . && dev-tool run bundle --polyfill-node false --inject-node-polyfills true --ignore-missing-node-builtins true",
    "build:browser": "npm run bundle",
    "build:node": "npm run bundle",
    "build:test": "echo skipped. actual commands inlined in browser test scripts",
    "build": "npm run clean && tshy && dev-tool run extract-api",
    "check-format": "dev-tool run vendored prettier --list-different --config ../../../.prettierrc.json --ignore-path ../../../.prettierignore \"src/**/*.ts\" \"test/**/*.ts\" \"samples-dev/**/*.ts\" \"*.{js,json}\"",
    "clean": "rimraf --glob dist dist-* types *.tgz *.log coverage coverage-browser .nyc_output",
    "build:samples": "echo Obsolete.",
    "execute:samples": "dev-tool samples run samples-dev",
    "extract-api": "tsc -p . && dev-tool run extract-api",
    "format": "dev-tool run vendored prettier --write --config ../../../.prettierrc.json --ignore-path ../../../.prettierignore \"samples/**/*.{ts,js}\" \"src/**/*.ts\" \"test/**/*.ts\" \"samples-dev/**/*.ts\" \"*.{js,json}\"",
    "integration-test:browser": "karma start --single-run",
    "integration-test:node": "dev-tool run test:node-js-input --no-test-proxy \"dist-esm/test/internal/**/*.spec.js\" \"dist-esm/test/public/**/*.spec.js\"",
    "integration-test": "npm run integration-test:node && npm run integration-test:browser",
    "lint:fix": "eslint package.json api-extractor.json README.md src test --fix --fix-type [problem,suggestion]",
    "lint": "eslint package.json api-extractor.json README.md src test",
    "pack": "npm pack 2>&1",
    "perf-test:node": "tsc -p . --module \"commonjs\" && node dist-esm/test/perf/track-2/index.spec.js",
    "test:browser": "npm run clean && npm run bundle && npm run integration-test:browser",
    "test:node": "npm run clean && npm run integration-test:node",
    "test": "npm run test:node && npm run test:browser",
    "unit-test:browser": "npm run clean && tshy && dev-tool run build-test && dev-tool run test:vitest --no-test-proxy --browser",
    "unit-test:node": "dev-tool run test:vitest --no-test-proxy",
    "unit-test": "npm run unit-test:node && npm run unit-test:browser"
  },
  "sideEffects": false,
  "//metadata": {
    "constantPaths": [
      {
        "path": "src/util/constants.ts",
        "prefix": "version"
      }
    ]
  },
  "//sampleConfiguration": {
    "productName": "Azure Service Bus",
    "productSlugs": [
      "azure",
      "azure-service-bus"
    ],
    "requiredResources": {
      "Azure Service Bus": "https://docs.microsoft.com/azure/service-bus-messaging"
    },
    "skip": [
      "receiveMessagesLoop.js",
      "receiveMessagesStreaming.js",
      "useProxy.js",
      "administrationClient.js",
      "listingEntities.js",
      "sessionState.js",
      "session.js",
      "deferral.js",
      "sessionRoundRobin.js"
    ]
  },
  "dependencies": {
    "@azure/abort-controller": "^2.1.2",
    "@azure/core-amqp": "^4.2.2",
    "@azure/core-auth": "^1.3.0",
    "@azure/core-client": "^1.0.0",
    "@azure/core-rest-pipeline": "^1.1.0",
    "@azure/core-tracing": "^1.0.0",
    "@azure/core-paging": "^1.4.0",
    "@azure/core-util": "^1.1.1",
    "@azure/core-xml": "^1.0.0",
    "@azure/logger": "^1.0.0",
    "@types/is-buffer": "^2.0.0",
    "buffer": "^6.0.0",
    "is-buffer": "^2.0.3",
    "jssha": "^3.1.0",
    "long": "^5.2.0",
    "process": "^0.11.10",
    "tslib": "^2.2.0",
    "rhea-promise": "^3.0.0"
  },
  "devDependencies": {
    "@azure-tools/test-credential": "^2.0.0",
    "@azure-tools/test-recorder": "^4.1.0",
    "@azure-tools/test-utils": "^1.0.1",
    "@azure/dev-tool": "^1.0.0",
    "@azure/eslint-plugin-azure-sdk": "^3.0.0",
    "@azure/identity": "^4.2.1",
    "@microsoft/api-extractor": "^7.31.1",
    "@types/debug": "^4.1.4",
    "@types/node": "^18.0.0",
    "@types/uuid": "^8.0.0",
    "@types/ws": "^7.2.4",
    "@vitest/browser": "^2.0.5",
    "@vitest/coverage-istanbul": "^2.0.5",
    "cross-env": "^7.0.2",
    "debug": "^4.1.1",
    "dotenv": "^16.0.0",
    "eslint": "^9.9.0",
    "events": "^3.0.0",
    "https-proxy-agent": "^7.0.0",
    "moment": "^2.24.0",
    "playwright": "^1.46.1",
    "promise": "^8.0.3",
    "rimraf": "^5.0.5",
    "tshy": "^3.0.2",
    "typescript": "~5.5.3",
    "uuid": "^8.3.0",
    "vitest": "^2.0.5",
    "ws": "^8.0.0"
  },
  "type": "module",
  "tshy": {
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts"
    },
    "dialects": [
      "esm",
      "commonjs"
    ],
    "esmDialects": [
      "browser",
      "react-native"
    ],
    "selfLink": false
  }
}