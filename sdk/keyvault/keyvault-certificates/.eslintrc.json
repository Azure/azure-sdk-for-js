{
  "plugins": ["@azure/azure-sdk"],
  "extends": ["../../.eslintrc.old.json", "plugin:@azure/azure-sdk/recommended"],
  "parserOptions": {
    "project": "tsconfig.eslint.json"
  },
  "ignorePatterns": ["src/core"],
  "rules": {
