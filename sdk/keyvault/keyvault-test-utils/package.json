{
  "name": "@azure/keyvault-test-utils",
  "version": "1.0.0",
  "description": "This private library provides a place to store common code for the tests of the Azure KeyVault libraries for JavaScript/TypeScript",
  "scripts": {
    "extract-api": "tsc -p . && api-extractor run --local",
    "build": "npm run extract-api && npm run build:es6 && npm run build:nodebrowser",
    "build:es6": "tsc -p tsconfig.json",
    "build:nodebrowser": "rollup -c 2>&1",
    "build:test": "npm run build:es6 && rollup -c rollup.test.config.js 2>&1",
    "check-format": "prettier --list-different --config ../../.prettierrc.json \"src/**/*.ts\" \"test/**/*.ts\" \"*.{js,json}\"",
    "format": "prettier --write --config ../../.prettierrc.json \"src/**/*.ts\" \"test/**/*.ts\" \"*.{js,json}\"",
    "unit-test:browser": "npm run build:test && karma start --single-run",
    "unit-test:node": "mocha --require source-map-support/register --reporter mocha-multi --reporter-options spec=-,mocha-junit-reporter=- --timeout 5000000 --full-trace dist-test/index.node.js",
    "unit-test": "npm run unit-test:node && npm run unit-test:browser",
    "test": "npm run build:test && npm run unit-test",
    "clean": "rimraf dist dist-esm dist-test types *.log browser statistics.html coverage src/**/*.js test/**/*.js",
    "prebuild": "npm run clean"
  },
  "files": [
    "types/",
    "dist/",
    "dist-esm/",
    "src/"
  ],
  "repository": "github:Azure/azure-sdk-for-js",
  "keywords": [
    "azure",
    "keyvault",
    "test-utils"
  ],
  "author": "Microsoft Corporation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/azure/azure-sdk-for-js/issues"
  },
  "engine": {
    "node": ">=8.0.0"
  },
  "main": "./dist/index.js",
  "module": "dist-esm/src/index.js",
  "types": "./types/keyvault-test-utils.d.ts",
  "homepage": "https://github.com/azure/azure-sdk-for-js/tree/master/sdk/keyvault/keyvault-test-utils",
  "sideEffects": false,
  "private": true,
  "dependencies": {
    "@azure/core-http": "^1.0.0",
    "@opentelemetry/types": "^0.2.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.5.4",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-multi-entry": "^3.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "@types/chai": "^4.1.6",
    "@types/fs-extra": "^8.0.0",
    "@types/mocha": "^5.2.5",
    "assert": "^1.4.1",
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-no-only-tests": "^2.3.0",
    "eslint-plugin-promise": "^4.1.1",
    "fs-extra": "^8.1.0",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^3.0.0",
    "karma-coverage": "^2.0.0",
    "karma-edge-launcher": "^0.4.2",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-remap-coverage": "^0.1.5",
    "mocha": "^6.2.2",
    "mocha-junit-reporter": "^1.18.0",
    "mocha-multi": "^1.1.3",
    "nyc": "^14.0.0",
    "prettier": "^1.16.4",
    "rimraf": "^3.0.0",
    "rollup": "^1.16.3",
    "rollup-plugin-shim": "^1.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-visualizer": "^3.1.1",
    "shx": "^0.3.2",
    "ts-node": "^8.3.0",
    "typescript": "~3.7.5",
    "uglify-js": "^3.4.9"
  }
}
