    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@azure/dev-tool": "^1.0.0",
    "@azure/identity-cache-persistence": "^1.1.1",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@types/webpack": "^5.28.5",
    "@types/webpack-dev-middleware": "^5.3.0",
    "buffer": "^6.0.3",
