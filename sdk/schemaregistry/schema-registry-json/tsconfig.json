{
  "extends": "../../../tsconfig",
  "compilerOptions": {
    "paths": {
      "@azure/schema-registry-json": [
        "./src/index"
      ]
    },
    "lib": [
      "dom"
    ],
    "skipLibCheck": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "rootDir": "."
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.mts",
    "src/**/*.cts",
    "samples-dev/**/*.ts",
    "test/**/*.ts",
    "test/**/*.mts",
    "test/**/*.cts"
  ]
}
