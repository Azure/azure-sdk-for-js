{
  "name": "@azure/functions-custom-authentication-extension",
  "version": "1.1.0",
  "description": "Typescript Trigger SDK for Azure AD Authentication event custom extensions. Lets you focus on your business logic.",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint --ext .ts,.json src/",
    "lint:test": "eslint --ext .ts,.json test/",
    "lint:samples": "eslint --ext .ts,.js samples/",
    "build": "npm install && npm run clean && npm run lint && npx tsc && npm run stripInternalDocs && echo Done",
    "build:doc": "npm install && npm run clean && npm run lint && npx tsc --project tsconfig.doc.json && tsc --pretty -p tsconfig.doc.json && api-extractor run --local --verbose && npm run build",
    "build:samples": "npm --prefix samples run build",
    "build:prod": "npm install --production",
    "validate:samples": "npm run build && npm --prefix samples install && npm run build:samples",
    "build:nolint": "npm run clean && npm run stripInternalDocs && echo Done",
    "stripInternalDocs": "tsc --pretty -p tsconfig.json",
    "test": "mocha -r ts-node/register 'test/**/*.ts'",
    "test:nolint": "npm run build:nolint && npm test",
    "watch": "tsc --watch",
    "watch:test": "npm run test -- --watch",
    "docs": "typedoc",
    "e2etst": "npm run "
  },
  "prepublish": "build",
  "devDependencies": {
    "@azure/functions": "^1.2.3",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "chai": "^4.3.6",
    "eslint": "^8.11.0",
    "mocha": "^9.2.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.13",
    "typedoc-plugin-markdown": "^3.11.14",
    "typescript": "^4.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Azure/azure-functions-authentication-events-js.git"
  },
  "keywords": [
    "azure-functions",
    "Authentication",
    "Events",
    "Trigger",
    "TypeScript",
    "JavaScript"
  ],
  "author": "Microsoft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Azure/azure-functions-authentication-events-js/issues"
  },
  "homepage": "https://github.com/Azure/azure-functions-authentication-events-js#readme",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "tests"
  },
  "dependencies": {},
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
