{
  "compilerOptions": {
    "module": "ESNext",
    "target": "es6",
    "useDefineForClassFields": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "moduleResolution": "Node",
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noImplicitReturns": true,
    "noEmit": true
  },
  "include": [
    "src"
  ]
}
