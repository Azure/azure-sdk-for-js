{
  "name": "@azure/arm-compute-v2",
  "author": "Microsoft Corporation",
  "description": "Compute Client",
  "version": "1.0.0",
  "dependencies": {
    "@azure/core-http": "^1.1.4",
    "@azure/core-lro": "^1.0.1",
    "@azure/core-paging": "^1.1.1",
    "@azure/identity": "^1.2.3",
    "tslib": "^2.0.0"
  },
  "keywords": [
    "node",
    "azure",
    "typescript",
    "browser",
    "isomorphic"
  ],
  "license": "MIT",
  "main": "./dist/arm-compute.js",
  "module": "./esm/src/index.js",
  "types": "./esm/src/index.d.ts",
  "devDependencies": {
    "@azure/test-utils-recorder": "file:../../test-utils/recorder/azure-test-utils-recorder-1.0.0.tgz",
    "@types/source-map-support": "^0.5.3",
    "@microsoft/api-extractor": "7.9.10",
    "@types/mocha": "^8.2.1",
    "@types/node": "^8.0.0",
    "dotenv": "^8.2.0",
    "esm": "^3.2.18",
    "mkdirp": "^1.0.4",
    "mocha": "^7.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "nyc": "^14.0.0",
    "rollup": "^0.66.2",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "ts-node": "^8.3.0",
    "typescript": "4.1.2",
    "uglify-js": "^3.4.9"
  },
  "homepage": "https://github.com/Azure/azure-sdk-for-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Azure/azure-sdk-for-js.git"
  },
  "bugs": {
    "url": "https://github.com/Azure/azure-sdk-for-js/issues"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "esm/**/*.js",
    "esm/**/*.js.map",
    "esm/**/*.d.ts",
    "esm/**/*.d.ts.map",
    "src/**/*.ts",
    "README.md",
    "rollup.config.js",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "tsc && rollup -c rollup.config.js && npm run minify && mkdirp ./review &&  npm run extract-api",
    "minify": "uglifyjs -c -m --comments --source-map \"content='./dist/arm-compute.js.map'\" -o ./dist/arm-compute.min.js ./dist/arm-compute.js",
    "prepack": "npm install && npm run build",
    "integration-test:node": "mocha -r esm --require ts-node/register --timeout 1200000 --full-trace test/*.ts",
    "unit-test:node": "export TEST_MODE=playback && npm run integration-test:node",
    "test:node:record": "npm run integration-test:node",
    "clear-recordings": "rm -fr recordings",
    "extract-api": "api-extractor run --local"
  },
  "sideEffects": false,
  "autoPublish": true
}
